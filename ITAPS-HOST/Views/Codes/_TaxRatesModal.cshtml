<input id="codeId" value="" hidden />

<div id="modal-add-setup" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" aria-labelledby="modal-standard-title" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title" id="modal-income-title">Add Tax Rate</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" style="color: white">&times;</span>
                </button>
            </div> 
            <div class="modal-body" style="overflow-y: scroll; height: 340px;">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <form>
                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Code" class="col-form-label">
                                            Code
                                        </label>
                                        <input type="text" class="form-control" id="code" placeholder="">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="OfficeType" class="col-form-label">
                                            Tax Rate
                                        </label>
                                        <input type="text" class="form-control" id="taxRate" placeholder="">
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Code" class="col-form-label">
                                            Tax Holiday
                                        </label>
                                        <input type="text" class="form-control" id="givenTaxHoliday" placeholder="">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class="col-form-label">
                                            Status
                                        </label>
                                        <select type="text" id="status" class="form-control" required>
                                            <option value="A">Active</option>
                                            <option value="I">Inactive</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Code" class="col-form-label">
                                            Sector
                                        </label>
                                        <input type="text" class="form-control" id="sector" placeholder="">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="OfficeType" class="col-form-label">
                                            Location
                                        </label>
                                        <input type="text" class="form-control" id="bizLoc" placeholder="">
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Code" class="col-form-label">
                                            Tax Holiday Rate
                                        </label>
                                        <input type="text" class="form-control" id="holidayRate" placeholder="">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="OfficeType" class="col-form-label">
                                            No. of Years
                                        </label>
                                        <input type="text" class="form-control" id="holidayYears" placeholder="">
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="description" class="col-form-label">
                                            Description
                                        </label>
                                        <textarea class="form-control" rows="2" id="description" style="resize: none;" value=""></textarea>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="descriptionAccType" class="col-form-label">
                                            Notes
                                        </label>
                                        <textarea class="form-control" rows="2" id="notes" style="resize: none;" value=""></textarea>
                                    </div>
                                </div>


                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                <button type="button" id="submitSetup" class="btn btn-primary">Save</button>
                <button type="button" id="editBtn" class="btn btn-light" style="display: none">Edit</button>
            </div>
        </div>
    </div>
</div>