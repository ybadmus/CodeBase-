<input id="appId" value="@ViewBag.UserId " hidden />


<div id="tccDetailsView">
    <partial name="_TCCHeader" />
    <div class="card-body">
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="tccDetails" role="tabpanel" aria-labelledby="nav-profile-tab">
                <div class="form-horizontal">
                    <div class="form-row">
                        <div class="col-md-5">
                            <div class="bordered-group" id="" style="height: 400px">
                                <p class="card-title"><b>Application Details</b></p>
                                <form style="margin-bottom: 10px;">
                                    <div class="form-group row">
                                        <div class="col-md-12">
                                            <label for="Description" class=" col-form-label">Applicant Name</label>
                                            <input type="text" class="form-control" id="applicantName" placeholder="">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-6 formDiv">
                                            <label for="Code" class="col-form-label">
                                                TIN
                                            </label>
                                            <input type="text" class="form-control" id="applicantTIN" placeholder="">
                                        </div>
                                        <div class="col-md-6 formDiv">
                                            <label for="Code" class="col-form-label">
                                                Phone
                                            </label>
                                            <input type="text" class="form-control" id="applicantPhone" placeholder="">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-12">
                                            <label for="Description" class=" col-form-label">Email</label>
                                            <input type="text" class="form-control" id="applicantEmail" placeholder="">
                                        </div>
                                    </div>
                                    <hr />
                                    <div class="form-group row">
                                        <div class="col-md-12">
                                            <label for="Description" class=" col-form-label">Requesting Entity</label>
                                            <input type="text" class="form-control" id="requestEntityName" placeholder="">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-6 formDiv">
                                            <label for="Code" class="col-form-label">
                                                TIN
                                            </label>
                                            <input type="text" class="form-control" id="requestingEntityTin" placeholder="">
                                        </div>
                                        <div class="col-md-6 formDiv">
                                            <label for="Code" class="col-form-label">
                                                Phone
                                            </label>
                                            <input type="text" class="form-control" id="requestingEntityPhone" placeholder="">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-md-12">
                                            <label for="Description" class=" col-form-label">Email</label>
                                            <input type="text" class="form-control" id="requestingEntityEmail" placeholder="">
                                        </div>
                                    </div>
                                </form>

                            </div>
                        </div>

                        <div class="col-md-7">
                            <div class="bordered-group" id="" style="height: 400px">
                                <p class="card-title"><b>Messages & Atachment</b></p>
                                <div id="tabsContainer" class="tab" style="width: 100% !important; display: block;">
                                    <button class="tablinks active" onclick="openDivForTab(event, 'applicantColumn')"> APPLICANT</button>
                                    <button class="tablinks" onclick="openDivForTab(event, 'internalMessagesColumn')"> INTERNAL</button>
                                    <button class="tablinks" onclick="openDivForTab(event, 'attachmentColumn')"> FILES</button>
                                </div>
                                <div>
                                    <partial name="_TCCMessages" />
                                </div>
                            </div>

                        </div>
                    </div>
                    <button id="backToGrid" class="btn btn-primary"> Back</button>
                </div>
            </div>
        </div>
    </div>
</div>



