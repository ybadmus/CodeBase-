@{
    ViewData["Title"] = "RateAndCode";
}


@section css {
    <link href="~/css/codes/others.css" rel="stylesheet" />
}

<input id="serverUrl" value="@ViewBag.ServerUrl" hidden />

<div class="card">
    <div class="card-header">
        <div class="row">
            <div class="col-md-2 offset-md-10">
                <button id="BtnOpenAddModal" class="btn btn-primary btn-lg float-right" type="button"><i class='fa fa-plus-circle'></i> Add</button>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="row align-items-end" style="margin: 1px;">
            <div class="GridDiv">
                <div id="Grid"></div>
            </div>
        </div>
    </div>
</div>

@section modals{
    <partial name="_RateAndCodeModal" />
}

@section scripts {
    <script src="~/js/codes/ratesandcodes.js"></script>
}
