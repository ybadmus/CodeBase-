<div id="modal-add-setup" class="modal fade" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-standard-title" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title" id="modal-income-title">Add</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" style="color: white">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <form>
                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">WHT Code</label>
                                        <input type="text" class="form-control" id="whtCode" placeholder="" style="font-weight: 600">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Account & Residency</label>
                                        <select type="text" id="accountAndResidency" class="form-control" style="font-weight: 600" required></select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Tax Rate</label>
                                        <input type="text" class="form-control" id="taxRate" placeholder="" style="font-weight: 600">
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Status</label>
                                        <select type="text" id="status" class="form-control" style="font-weight: 600" required>
                                            <option value="A">Active</option>
                                            <option value="I">Inactive</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="Description" class=" col-form-label">Description</label>
                                        <input type="text" class="form-control" id="description" placeholder="" style="font-weight: 600">
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="Note" class="col-form-label">
                                            Notes
                                        </label>
                                        <textarea class="form-control" rows="3" id="notes" style="font-weight: 600; resize: none;" value=""></textarea>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                <button type="button" id="SubmitSetup" class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>

<div id="modal-edit-setup" class="modal fade" tabindex="-1" data-backdrop="static" role="dialog" aria-labelledby="modal-standard-title" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title" id="modal-income-title">Update <span id="EditModalSetupName"></span></h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" style="color: white">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <form>
                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">WHT Code</label>
                                        <input type="text" class="form-control" id="whtCodeUpdate" placeholder="" disabled>
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Account & Residency</label>
                                        <select type="text" id="accountAndResidencyUpdate" class="form-control" disabled></select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Tax Rate</label>
                                        <input type="text" class="form-control" id="taxRateUpdate" placeholder="" disabled>
                                    </div>
                                    <div class="col-md-6 formDiv">
                                        <label for="Status" class=" col-form-label">Status</label>
                                        <select type="text" id="statusUpdate" class="form-control" disabled>
                                            <option value="A">Active</option>
                                            <option value="I">Inactive</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="Description" class=" col-form-label">Description</label>
                                        <input type="text" class="form-control" id="descriptionUpdate" placeholder="" disabled>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <label for="Note" class="col-form-label">
                                            Notes
                                        </label>
                                        <textarea class="form-control" rows="3" id="noteUpdate" style="resize: none;" value="" disabled></textarea>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                <button type="button" id="BtnEdit" class="btn btn-light"><i class="fa fa-lock" aria-hidden="true"></i>&nbsp;&nbsp; Edit</button>
                <button type="button" id="BtnUpdate" class="btn btn-primary" style="display:none">Save</button>
            </div>
        </div>
    </div>
</div>