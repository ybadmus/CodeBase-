<div id="modal-add-setup" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="modal-standard-title" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title" id="modal-income-title">Add</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" style="color: white">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <form>
                                <div class="form-group row">
                                    <div class="col-md-12 formDiv">
                                        <h6>Please are you sure all details provided are accurate ?</h6>
                                        <label for="codeTR" class="col-form-label">
                                            Code
                                        </label>
                                        <input type="text" class="form-control" id="codeTR" placeholder="">
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-6  formDiv">
                                        <label for="startDateTR" class="col-form-label">
                                            <b>Start Date</b> (Required)
                                        </label>
                                        <input id="startDateTR" type="text" class="form-control" placeholder="Expiry" data-toggle="flatpickr" value="today">
                                    </div>
                                    <div class="col-md-6  formDiv">
                                        <label for="endDateTR" class="col-form-label">
                                            <b>End Date</b> (Required)
                                        </label>
                                        <input id="endDateTR" type="text" class="form-control" placeholder="Expiry" data-toggle="flatpickr" value="today">
                                    </div>
                                </div>
                                <br />
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                <button type="button" id="continueSubmit" class="btn btn-success">Continue</button>
            </div>
        </div>
    </div>
</div>